{
  "name": "notary-client",
  "version": "0.0.1",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "gh-pages": "^2.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "prettier": "^1.17.1",
    "prettier-plugin-svelte": "^0.5.1",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production ORBS_NODE_ADDRESS=https://validator.orbs-test.com/vchains/6666 ORBS_VCHAIN=6666 ORBS_PRISM_URL=http://prism.orbs.network webpack",
    "dev": "cross-env ORBS_NODE_ADDRESS=http://localhost:8080 ORBS_VCHAIN=42 ORBS_PRISM_URL=http://localhost:3000 webpack-dev-server --content-base public --port 9000",
    "lint": "prettier --write 'src/**/*.{js,svelte}'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "gamma:start": "gamma-cli start-local",
    "gamma:stop": "gamma-cli stop-local",
    "notary:local": "gamma-cli deploy ./contract/notary.go -name Notary -signer user1 -env local",
    "notary:testnet": "gamma-cli deploy ./contract/notary.go -name Notary -signer user1 -env validator",
    "test": "mocha test"
  },
  "dependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^6.1.4",
    "orbs-client-sdk": "0.0.6",
    "sjcl": "^1.0.8"
  },
  "prettier": {
    "singleQuote": true
  }
}
